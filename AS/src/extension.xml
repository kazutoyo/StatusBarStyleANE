  <versionNumber>1</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libStatusBarStyleANE.a</nativeLibrary>
                <initializer>StatusBarStyleANEExtInitializer</initializer>
                <finalizer>StatusBarStyleANEExtFinalizer</finalizer>
            </applicationDeployment>
    </platform>
    
    <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libStatusBarStyleANE.a</nativeLibrary>
